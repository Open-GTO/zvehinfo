/*
	Vehicle Type functions
*/

stock GetVehicleModelType(model)
{
	if (611 < model < 400) {
		return -1;
	}

	return gVehicleInfo[model - 400][e_zviType];
}

stock GetVehicleType(vehicleid)
{
	return GetVehicleModelType( GetVehicleModel(vehicleid) );
